<div class="wrapper">
    <form action="" id="signup-form">
        <div id="wizard">
          <h4></h4>
          <section>
            <div class="form-header">
              <div class="avartar">
                <h3 class="mb-4 text-center">Create an Account</h3>
              </div>
              <div class="form-group">
                <div class="form-holder active">
                  <input type="text" placeholder="First Name" class="form-control" id="first-name">
                </div>
                <div class="form-holder">
                  <input type="text" placeholder="Last Name" class="form-control" id="last-name">
                </div>
                <div class="form-holder">
                  <input id="username-signup" type="text" placeholder="Username" class="form-control">
                </div>
              </div>
            </div>
            <div class="form-holder">
              <input id="email-signup" type="text" placeholder="Email" class="form-control">
            </div>
            <div class="form-holder">
              <input id="password-signup" type="password" placeholder="Create a password" class="form-control">
            </div>
            <button type="submit" class="form-control btn btn-primary submit px-3">Sign Up</button>
          </section>
        </div>
    </form>
  </div>

  <script src="/js/login.js"></script>
